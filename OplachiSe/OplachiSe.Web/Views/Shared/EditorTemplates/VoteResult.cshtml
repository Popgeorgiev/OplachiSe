﻿@model Int32
<div class="row">
    <div class="col-md-2">
        @Html.LabelFor(m => m)
    </div>
    <div class="col-md-5">
        @Html.DropDownListFor(m => m, Enumerable.Range(1, 5).Select(x => new SelectListItem { Text = x.ToString() }))
    </div>
    <div>
        @Html.ValidationMessageFor(m => m)
    </div>
</div>
<br />